  "slides":["<!doctype html>\n<html lang=\"pt-br\">\n\n<head>\n  <meta charset=\"utf-8\" />\n  <title>Template 6 – Slide 1</title>\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\" />\n  <link href=\"https://fonts.googleapis.com/css2?family=Montserrat:wght@600;700&display=swap\" rel=\"stylesheet\">\n  <style>\n    * {\n      box-sizing: border-box;\n      margin: 0;\n      padding: 0\n    }\n\n    :root {\n      --w: 1080px;\n      --h: 1350px\n    }\n\n    .slide {\n      width: var(--w);\n      height: var(--h);\n      position: relative;\n      overflow: hidden;\n      background: #000;\n      color: #fff;\n      font-family: \"Montserrat\", Arial, sans-serif;\n      --header-color: #ffffff;\n      /* cor padrão do topo */\n    }\n\n    .bg {\n      position: absolute;\n      inset: 0;\n      background-image:\n        linear-gradient(to top, rgba(0, 0, 0, .92) 0%, rgba(0, 0, 0, .62) 40%, rgba(0, 0, 0, 0) 60%),\n        url(\"https://admin.cnnbrasil.com.br/wp-content/uploads/sites/12/2025/01/Santos-Neymar-braco-Cruzado.jpg\");\n      background-size: cover;\n      background-position: center;\n    }\n\n    /* aplica a cor dinâmica no topo */\n    .slide>.handle,\n    .slide>.name {\n      color: var(--header-color);\n    }\n\n    .content {\n      position: relative;\n      width: 100%;\n      height: 100%;\n      display: flex;\n      flex-direction: column;\n      align-items: center;\n      justify-content: flex-end;\n      padding: 72px 48px;\n      gap: 18px;\n      text-align: center;\n    }\n\n    .profile {\n      display: inline-flex;\n      align-items: center;\n      gap: 14px;\n    }\n\n    .avatar {\n      width: 72px;\n      height: 72px;\n      border-radius: 50%;\n      border: 3px solid #1ACD8A;\n      object-fit: cover;\n      flex: 0 0 auto;\n    }\n\n    .name {\n      font-weight: 700;\n      font-size: 22px;\n      line-height: 1.2;\n    }\n\n    .handle {\n      font-weight: 600;\n      font-size: 18px;\n      opacity: .95;\n    }\n\n    .title {\n      font-weight: 700;\n      font-size: 60px;\n      line-height: 1.2;\n      letter-spacing: -0.02em;\n      max-width: min(880px, 90%);\n      white-space: normal;\n      overflow-wrap: anywhere;\n      word-break: break-word;\n      hyphens: auto;\n      text-align: center;\n    }\n  </style>\n</head>\n\n<body>\n  <main class=\"slide\">\n    <div class=\"bg\" aria-hidden=\"true\"></div>\n\n    <!-- Topo -->\n    <div class=\"handle\" style=\"position:absolute; top:36px; left:60px;\">@{{arroba}}</div>\n    <div class=\"name\" style=\"position:absolute; top:36px; right:60px; font-weight:700;\">Copyright ©</div>\n\n    <!-- Bloco sobre o gradiente -->\n    <section class=\"content\">\n      <div class=\"profile\">\n        <img class=\"avatar\" src=\"https://images.unsplash.com/photo-1502685104226-ee32379fefbe?q=80&w=256&auto=format&fit=crop\" alt=\"Avatar\" />\n        <div style=\"display:flex;flex-direction:column;align-items:flex-start;\">\n          <div class=\"name\">{{nome}}</div>\n          <div class=\"handle\">@{{arroba}}</div>\n        </div>\n      </div>\n      <div class=\"title\">{{title}}</div>\n    </section>\n  </main>\n\n  <script>\n    (function () {\n    const slide = document.querySelector('.slide');\n    const bg    = slide.querySelector('.bg');\n    const topLeft  = slide.querySelector(':scope > .handle');\n    const topRight = slide.querySelector(':scope > .name');\n    if (!bg || !topLeft || !topRight) return;\n\n    // última url(...) do background-image (a imagem real)\n    const bgImg = getComputedStyle(bg).backgroundImage;\n    const matches = [...bgImg.matchAll(/url\\((?:'|\")?([^\"')]+)(?:'|\")?\\)/g)];\n    const url = matches.length ? matches[matches.length - 1][1] : null;\n    if (!url) return;\n\n    const img = new Image();\n    img.crossOrigin = 'anonymous';\n    img.onload = () => {\n      try {\n        const W = 128, H = 128;\n        const c = document.createElement('canvas'); c.width = W; c.height = H;\n        const ctx = c.getContext('2d', { willReadFrequently: true });\n        ctx.drawImage(img, 0, 0, W, H);\n\n        // luminância média da faixa superior (12%)\n        const bandH = Math.max(1, Math.round(H * 0.12));\n        const data = ctx.getImageData(0, 0, W, bandH).data;\n        let sum = 0, n = 0;\n        for (let i = 0; i < data.length; i += 4) {\n          const r = data[i], g = data[i+1], b = data[i+2];\n          sum += 0.2126*r + 0.7152*g + 0.0722*b;\n          n++;\n        }\n        const avg = sum / n; // 0..255\n\n        // Se a área é clara, usa #37474F (em vez de preto)\n        if (avg > 200) slide.style.setProperty('--header-color', '#37474F');\n      } catch (e) {\n        // Sem CORS: fallback para legibilidade automática\n        [topLeft, topRight].forEach(el => {\n          el.style.mixBlendMode = 'difference';\n          el.style.color = '#ffffff';\n        });\n      }\n    };\n    img.onerror = () => {\n      [topLeft, topRight].forEach(el => {\n        el.style.mixBlendMode = 'difference';\n        el.style.color = '#ffffff';\n      });\n    };\n    img.src = url;\n  })();\n  </script>\n</body>\n\n</html>","<!doctype html>\n<html lang=\"pt-br\">\n\n<head>\n  <meta charset=\"utf-8\" />\n  <title>Template 6 – Slide 2</title>\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\" />\n  <link href=\"https://fonts.googleapis.com/css2?family=Montserrat:wght@400;600;700&display=swap\" rel=\"stylesheet\">\n  <style>\n    * {\n      box-sizing: border-box;\n      margin: 0;\n      padding: 0\n    }\n\n    :root {\n      --w: 1080px;\n      --h: 1350px\n    }\n\n    .slide {\n      width: var(--w);\n      height: var(--h);\n      background: #F1F1F1;\n      color: #37474F;\n      font-family: \"Montserrat\", Arial, sans-serif;\n      position: relative;\n      overflow: hidden;\n      display: flex;\n      flex-direction: column;\n    }\n\n    /* Top labels */\n    .slide>.handle.top {\n      position: absolute;\n      top: 36px;\n      left: 60px;\n      color: #848484;\n      font-weight: 600;\n    }\n\n    .slide>.name.top {\n      position: absolute;\n      top: 36px;\n      right: 60px;\n      color: #848484;\n      font-weight: 700;\n    }\n\n    /* Grid layout */\n    .content {\n      width: 100%;\n      height: 100%;\n      display: flex;\n      flex-direction: column;\n      padding: 120px 60px 60px;\n      gap: 28px;\n      justify-content: center;\n    }\n\n    .text-box {\n      display: flex;\n      flex-direction: column;\n      gap: 12px;\n      align-items: flex-start;\n    }\n\n    .title {\n      font-weight: 700;\n      font-size: 67px;\n      line-height: 1.18;\n      letter-spacing: -0.02em;\n      max-width: 88%;\n      text-align: left;\n      color: #37474F;\n      white-space: normal;\n      overflow-wrap: anywhere;\n      word-break: break-word;\n    }\n\n    .subtitle {\n      font-weight: 400;\n      font-size: 42px;\n      line-height: 1.22;\n      color: #37474F;\n      max-width: 88%;\n      text-align: left;\n      white-space: normal;\n      overflow-wrap: anywhere;\n      word-break: break-word;\n      margin-bottom: 30px;\n    }\n\n    /* Image */\n    .text-box+img {\n      width: 100%;\n      height: 450px;\n      object-fit: cover;\n      object-position: top;\n      border-radius: 24px;\n      display: block;\n      box-shadow: 0 16px 48px rgba(0, 0, 0, .18);\n    }\n\n    /* Footer */\n    .footer {\n      width: 100%;\n      display: flex;\n      align-items: center;\n      justify-content: space-between;\n      padding: 36px 60px;\n    }\n\n    .profile {\n      display: inline-flex;\n      align-items: center;\n      gap: 14px;\n    }\n\n    .avatar {\n      width: 64px;\n      height: 64px;\n      border-radius: 50%;\n      border: 3px solid #1ACD8A;\n      object-fit: cover;\n      flex: 0 0 auto;\n    }\n\n    .name {\n      font-weight: 700;\n      font-size: 24px;\n      line-height: 1.2;\n    }\n\n    .handle {\n      font-weight: 600;\n      font-size: 18px;\n    }\n\n    /* >>> CORREÇÃO: nome e arroba do BLOCO INFERIOR em #8D8D8D */\n    .footer .name,\n    .footer .handle {\n      color: #8D8D8D;\n      opacity: 1;\n    }\n  </style>\n</head>\n\n<body>\n  <main class=\"slide\">\n    <!-- Top -->\n    <div class=\"handle top\">@{{arroba}}</div>\n    <div class=\"name top\">Copyright ©</div>\n\n    <!-- Content -->\n    <section class=\"content\">\n\n      <div>\n        <div class=\"text-box\">\n          <div class=\"title\">{{title}}</div>\n          <div class=\"subtitle\">{{subtitle}}</div>\n        </div>\n\n        <img src=\"https://admin.cnnbrasil.com.br/wp-content/uploads/sites/12/2025/01/Santos-Neymar-braco-Cruzado.jpg\" alt=\"\">\n      </div>\n\n    </section>\n\n    <div class=\"footer\">\n      <div class=\"profile\">\n        <img class=\"avatar\" src=\"https://images.unsplash.com/photo-1502685104226-ee32379fefbe?q=80&w=256&auto=format&fit=crop\" alt=\"Avatar\" />\n        <div style=\"display:flex;flex-direction:column;align-items:flex-start;\">\n          <div class=\"name\">{{nome}}</div>\n          <div class=\"handle\">@{{arroba}}</div>\n        </div>\n      </div>\n\n      <div style=\"color:#848484; font-weight:600; font-size:18px;\">Arrasta para o lado &gt;</div>\n    </div>\n\n  </main>\n</body>\n\n</html>","<!doctype html>\n<html lang=\"pt-br\">\n\n<head>\n  <meta charset=\"utf-8\" />\n  <title>Template 6 – Slide 2</title>\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\" />\n  <link href=\"https://fonts.googleapis.com/css2?family=Montserrat:wght@400;600;700&display=swap\" rel=\"stylesheet\">\n  <style>\n    * {\n      box-sizing: border-box;\n      margin: 0;\n      padding: 0\n    }\n\n    :root {\n      --w: 1080px;\n      --h: 1350px\n    }\n\n    .slide {\n      width: var(--w);\n      height: var(--h);\n      background: #F1F1F1;\n      color: #37474F;\n      font-family: \"Montserrat\", Arial, sans-serif;\n      po